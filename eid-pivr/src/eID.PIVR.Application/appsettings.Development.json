{
  "Serilog": {
    "MinimumLevel": {
      "Default": "Debug",
      "Override": {
        "System": "Information",
        "Microsoft": "Information"
      }
    },
    "Properties": {
      "EnvironmentSource": "xxx-Local"
    },
    "Using": [
      "Serilog.Expressions"
    ],
    "Filter": [
      {
        "Name": "ByExcluding",
        "Args": {
          "expression": "RequestPath like '/health%' and @l = 'Information'"
        }
      },
      {
        "Name": "ByExcluding",
        "Args": {
          "expression": "EndsWith(RequestPath, '/metrics')"
        }
      }
    ],
    "WriteTo": [
      {
        "Name": "Console",
        "Args": {
          "outputTemplate": "{Timestamp:yyyy-MM-dd HH:mm:ss.ff zzz} [{Level:u4}] {Message}{NewLine}{Exception}"
        }
      },
      {
        "Name": "File",
        "Args": {
          "path": "logs/eID-PIVR-Application.log",
          "fileSizeLimitBytes": 10485760,
          "rollOnFileSizeLimit": true,
          "retainedFileCountLimit": 10,
          "outputTemplate": "{Timestamp:yyyy-MM-dd HH:mm:ss.ff zzz} [{Level:u4}] {Message}{NewLine}{Exception}"
        }
      }
    ]
  },
  "RabbitMqTransportOptions": {
    // Default RabbitMqTransportOptions values match default RabbitMQ configuration.
    // No additional configuration should be required for local hosting
    "Host": "localhost",
    "Port": 5672,
    "User": "guest",
    "Pass": "guest",
    "ManagementPort": 15672,
    "VHost": "/"
  },
  "RedisOptions": {
    "ConnectionString": "localhost:6379,password=passw@rd"
    // Default RedisOptions values match default Redis configuration.
    // No additional configuration should be required for local hosting
  },
  "ConnectionStrings": {
    "DefaultConnection": "Host=localhost;Database=eid-pivr;Username=postgres;Password=eid$pass"
  },
  "RegiXOptions": {
    "ServiceUrl": "http://localhost:8002/integrations/regix/",
    "ConcurrentMessageLimit": 1,
    "SocketsHttpHandlerConnectTimeoutInMs": 200,
    "UseProductionService": false
  },
  "ApplicationUrls": {
    "IntegrationsHostUrl": "http://localhost:8002/integrations"
  },
  "USE_REGIX_FOR_MVR": false,
  "ExternalRegistersCacheOptions": {
    "ExpireAfterInHours": 6
  }
}