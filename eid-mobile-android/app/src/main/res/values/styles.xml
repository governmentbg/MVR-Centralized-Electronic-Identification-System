    </style>

    <!-- Tab layout -->

    <style name="AppTabLayout" parent="Widget.Design.TabLayout">
        <item name="tabGravity">fill</item>
        <item name="tabIndicator">@null</item>
        <item name="tabMaxWidth">0dp</item>
        <item name="tabIndicatorHeight">4dp</item>
        <item name="tabMode">fixed</item>
        <item name="tabIndicatorFullWidth">true</item>
        <item name="android:tabStripEnabled">true</item>
        <item name="tabRippleColor">@android:color/transparent</item>
        <item name="tabTextColor">@color/selector_tab_text_color</item>
        <item name="tabTextAppearance">@style/AppTabTextAppearance</item>
    </style>

    <style name="AppTabTextAppearance" parent="TextAppearance.Design.Tab">
        <item name="android:textSize">14sp</item>
        <item name="android:textColor">@color/selector_tab_text_color</item>
        <item name="fontFamily">@font/inter_regular</item>
        <item name="fontWeight">400</item>
        <item name="textAllCaps">false</item>
    </style>

