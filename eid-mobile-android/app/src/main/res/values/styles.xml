<?xml version="1.0" encoding="utf-8"?>
<resources xmlns:tools="http://schemas.android.com/tools">

    <style name="Theme.App.Starting" parent="Theme.SplashScreen">
        <item name="android:statusBarColor">@color/color_295999</item>
        <item name="android:navigationBarColor">@color/color_1C3050</item>
        <item name="android:windowLightStatusBar">true</item>
        <item name="windowSplashScreenBackground">@color/color_white</item>
        <item name="windowSplashScreenAnimatedIcon">@drawable/ic_splash_logo</item>
        <item name="postSplashScreenTheme">@style/Theme.App</item>
        <item name="android:forceDarkAllowed" tools:targetApi="29">false</item>
        <item name="android:windowFullscreen">true</item>
        <item name="android:windowContentOverlay">@null</item>
        <item name="android:windowActionBar">false</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowTranslucentNavigation">true</item>
    </style>

    <style name="Theme.App" parent="Theme.MaterialComponents.Light.NoActionBar">
        <!--        <item name="android:background">@color/color_white</item>-->
        <item name="android:statusBarColor">@color/color_295999</item>
        <item name="android:navigationBarColor">@color/color_1C3050</item>
        <item name="colorPrimary">@color/color_295999</item>
        <item name="colorPrimaryVariant">@color/color_295999</item>
        <item name="colorOnPrimary">@color/color_white</item>
        <item name="colorPrimaryDark">@color/color_295999</item>
        <item name="colorSecondary">@color/color_295999</item>
        <item name="colorSecondaryVariant">@color/color_295999</item>
        <item name="colorOnSecondary">@color/color_295999</item>
        <item name="colorAccent">@color/color_295999</item>
        <item name="android:colorEdgeEffect">@color/color_295999</item>
        <item name="android:windowBackground">@color/color_white</item>
        <item name="android:windowLightStatusBar">true</item>
        <item name="bottomSheetDialogTheme">@style/AppBottomSheetDialogTheme</item>
        <item name="windowActionModeOverlay">true</item>
        <item name="android:forceDarkAllowed" tools:targetApi="29">false</item>
        <item name="android:windowContentOverlay">@null</item>
        <item name="android:windowActionBar">false</item>
        <item name="android:windowNoTitle">true</item>
    </style>

    <style name="CustomListPopupWindowStyle" parent="Widget.AppCompat.ListPopupWindow">
        <item name="android:popupBackground">@drawable/bg_popup_window_menu</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
    </style>

    <style name="BottomNavigationViewStyle" parent="Widget.MaterialComponents.BottomNavigationView">
        <item name="android:background">@color/color_295999</item>
        <item name="itemRippleColor">@color/color_ripple</item>
        <item name="itemTextAppearanceActive">@style/TextStyle.14sp.Regular.1C3050</item>
        <item name="itemTextAppearanceInactive">@style/TextStyle.14sp.Regular.1C3050</item>
        <item name="itemTextColor">@color/selector_bottom_navigation</item>
        <item name="labelVisibilityMode">labeled</item>
        <item name="itemIconTint">@color/selector_bottom_navigation</item>
    </style>

    <style name="AppModalStyle" parent="Widget.Design.BottomSheet.Modal">
        <item name="android:background">@drawable/bg_bottom_sheet_base</item>
        <item name="behavior_peekHeight">300dp</item>
    </style>

    <style name="AppBottomSheetDialogTheme" parent="Theme.Design.Light.BottomSheetDialog">
        <item name="bottomSheetStyle">@style/AppModalStyle</item>
        <item name="colorAccent">@color/color_295999</item>
    </style>

    <style name="BannerMessageStyle">
        <item name="android:windowEnterAnimation">@anim/banner_slide_down</item>
        <item name="android:windowExitAnimation">@anim/banner_slide_up</item>
    </style>

    <!-- Spinner -->

    <style name="SpinnerStyle" parent="TextStyle.16sp.Regular.0C53B2">
        <item name="android:fontFamily">@font/sofia_sans_regular</item>
        <item name="android:textColor">@color/color_1C3050</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:paddingTop">@dimen/margin_8</item>
        <item name="android:paddingBottom">@dimen/margin_8</item>
        <item name="android:paddingStart">@dimen/margin_8</item>
        <item name="android:paddingEnd">@dimen/margin_8</item>
        <item name="android:background">@drawable/bg_text</item>
    </style>

    <style name="SpinnerNotEditableStyle" parent="SpinnerStyle">
        <item name="android:background">@drawable/bg_text_not_editable</item>
    </style>

    <!-- Buttons -->

    <style name="ButtonStyle">
        <item name="android:paddingTop">@dimen/margin_5</item>
        <item name="android:paddingBottom">@dimen/margin_5</item>
        <item name="android:paddingStart">@dimen/margin_18</item>
        <item name="android:paddingEnd">@dimen/margin_18</item>
        <item name="android:layout_gravity">bottom</item>
        <item name="android:clickable">true</item>
        <item name="android:focusable">true</item>
        <item name="android:gravity">center</item>
        <item name="android:includeFontPadding">false</item>
        <item name="android:textAllCaps">false</item>
        <item name="android:stateListAnimator">@null</item>
        <item name="android:elevation">0dp</item>
        <item name="cornerRadius">4dp</item>
    </style>

    <style name="ButtonStyle.background_1C3050_text_white">
